.MODEL SMALL
.STACK 100H
.DATA
N DW 9
NEWLINE DB 0DH, 0AH, "$"
.CODE
MAIN PROC
MOV AX, @DATA
MOV DS, AX
XOR BX, 48
MOV AX, N
MOV CX,AX
START:

MOV AH, 02H
MOV DX,BX
INT 21H

MOV AH, 09H
LEA DX,NEWLINE
INT 21H
INC  BX


loop start

MOV AH, 02H
INT 21H

MAIN ENDP
END MAIN




; [SOURCE]: C:\emu8086\MySource\LOOP.asm
